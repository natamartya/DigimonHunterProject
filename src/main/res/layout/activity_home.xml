<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".home">

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="427dp"
        android:layout_height="98dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/atas" />

    <TextView
        android:id="@+id/welcomeTxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Welcome,\nGuest"
        android:textColor="#FFFFFF"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="20dp"
        tools:layout_editor_absoluteY="19dp" />

    <ImageButton
        android:id="@+id/logoutBtn"
        android:layout_width="61dp"
        android:layout_height="61dp"
        android:background="#fffff"
        android:elevation="4dp"
        android:scaleType="centerInside"
        android:src="@drawable/logoutt"
        app:layout_constraintBottom_toBottomOf="@+id/imageView3"
        app:layout_constraintEnd_toEndOf="@+id/imageView3"
        app:layout_constraintHorizontal_bias="0.973"
        app:layout_constraintStart_toEndOf="@+id/welcomeTxt"
        app:layout_constraintTop_toTopOf="@+id/imageView3"
        app:layout_constraintVertical_bias="0.225" />

    <Button
        android:id="@+id/bronzeBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#FDB435"
        android:text="Bronze"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.226"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3"
        app:layout_constraintVertical_bias="0.032" />

    <Button
        android:id="@+id/silverBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#FDB435"
        android:text="Silver"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/bronzeBtn"
        app:layout_constraintTop_toBottomOf="@+id/imageView3"
        app:layout_constraintVertical_bias="0.032" />

    <Button
        android:id="@+id/goldBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#F99D1B"
        android:text="Gold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/silverBtn"
        app:layout_constraintTop_toBottomOf="@+id/imageView3"
        app:layout_constraintVertical_bias="0.032" />

    <ImageView
        android:id="@+id/bronzediamond"
        android:layout_width="122dp"
        android:layout_height="199dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/silverBtn"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/bronze" />

    <TextView
        android:id="@+id/bronzeTxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif"
        android:textStyle="bold"
        android:textColor="#5B2A03"
        android:text="Bronze Membership's Benefits"
        android:textSize="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bronzediamond"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/bcheck1"
        android:layout_width="39dp"
        android:layout_height="51dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.196"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bronzeTxt"
        app:layout_constraintVertical_bias="0.045"
        app:srcCompat="@drawable/centang" />

    <TextView
        android:id="@+id/benone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Updated news on digimon sales"
        android:textColor="#000000"
        android:textSize="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.207"
        app:layout_constraintStart_toEndOf="@+id/bcheck1"
        app:layout_constraintTop_toBottomOf="@+id/bronzeTxt"
        app:layout_constraintVertical_bias="0.074" />

    <ImageView
        android:id="@+id/brcheck2"
        android:layout_width="39dp"
        android:layout_height="51dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.196"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bcheck1"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/centang" />

    <TextView
        android:id="@+id/bentwo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="5% off every purchase"
        android:textColor="#000000"
        android:textSize="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.122"
        app:layout_constraintStart_toEndOf="@+id/brcheck2"
        app:layout_constraintTop_toBottomOf="@+id/benone"
        app:layout_constraintVertical_bias="0.094" />

    <ImageView
        android:id="@+id/brcheck3"
        android:layout_width="39dp"
        android:layout_height="51dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.196"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/brcheck2"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/centang" />

    <TextView
        android:id="@+id/benthree"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="No advertisement"
        android:textColor="#000000"
        android:textSize="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.104"
        app:layout_constraintStart_toEndOf="@+id/brcheck3"
        app:layout_constraintTop_toBottomOf="@+id/bentwo"
        app:layout_constraintVertical_bias="0.108" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="429dp"
        android:layout_height="353dp"
        android:layout_marginTop="92dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bentwo"
        app:srcCompat="@drawable/bawah" />

    <ImageButton
        android:id="@+id/homeBtn"
        android:layout_width="61dp"
        android:layout_height="61dp"
        android:background="#fffff"
        android:elevation="4dp"
        android:scaleType="centerInside"
        android:src="@drawable/homeorgg"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.115"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/welcomeTxt"
        app:layout_constraintVertical_bias="0.976" />

    <ImageButton
        android:id="@+id/digimonBtn"
        android:layout_width="61dp"
        android:layout_height="61dp"
        android:background="#fffff"
        android:elevation="4dp"
        android:scaleType="centerInside"
        android:src="@drawable/digimon"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.305"
        app:layout_constraintStart_toEndOf="@+id/homeBtn"
        app:layout_constraintTop_toBottomOf="@+id/bentwo"
        app:layout_constraintVertical_bias="0.927" />

    <ImageButton
        android:id="@+id/contactusBtn"
        android:layout_width="61dp"
        android:layout_height="61dp"
        android:background="#fffff"
        android:elevation="4dp"
        android:scaleType="centerInside"
        android:src="@drawable/contactuss"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageView9"
        app:layout_constraintHorizontal_bias="0.65"
        app:layout_constraintStart_toEndOf="@+id/digimonBtn"
        app:layout_constraintTop_toBottomOf="@+id/logoutBtn"
        app:layout_constraintVertical_bias="0.982" />

</androidx.constraintlayout.widget.ConstraintLayout>